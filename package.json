{
    "name": "vscodex",
    "displayName": "Code:X",
    "version": "0.0.12",
    "author": "Julien Wintz",
    "main": "./out/extension.js",
    "engines": {
        "vscode": "^1.94.0"
    },
    "icon": "package.png",
    "description": "Personal extension framework",
    "publisher": "jwintz",
    "repository": {
        "type": "git",
        "url": "https://github.com/jwintz/vscodex"
    },
    "keywords": [
        "codex",
        "leader",
        "emacs"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "license": "MIT",
    "contributes": {
        "commands": [
            {
                "command": "codex.command.indentBuffer",
                "category": "Code:X",
                "title": "Indent Buffer"
            },
            {
                "command": "codex.command.indentRegion",
                "category": "Code:X",
                "title": "Indent Region"
            },
            {
                "command": "codex.command.commentRegion",
                "category": "Code:X",
                "title": "Comment Region"
            },
            {
                "command": "codex.command.uncommentRegion",
                "category": "Code:X",
                "title": "Uncomment Region"
            },
            {
                "command": "codex.command.makeHeader",
                "category": "Code:X",
                "title": "Make Header"
            },
            {
                "command": "codex.command.makeSection",
                "category": "Code:X",
                "title": "Make Section"
            },
            {
                "command": "codex.command.makeDivider",
                "category": "Code:X",
                "title": "Make Divider"
            }
        ],
        "configuration": {
            "title": "Code:X",
            "properties": {
                "codex.whichkey.bindings": {
                    "type": "array",
                    "default": [
                        {
                            "key": "c",
                            "name": "Copilot",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "c",
                                    "name": "Copilot Chat",
                                    "type": "command",
                                    "command": "workbench.panel.chat"
                                },
                                {
                                    "key": "i",
                                    "name": "Copilot Chat inline",
                                    "type": "command",
                                    "command": "inlineChat.startWithCurrentLine"
                                }
                            ]
                        },
                        {
                            "key": "d",
                            "name": "Debug",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "d",
                                    "name": "Debug start",
                                    "type": "command",
                                    "command": "workbench.action.debug.start"
                                },
                                {
                                    "key": "D",
                                    "name": "Debug stop",
                                    "type": "command",
                                    "command": "workbench.action.debug.stop"
                                },
                                {
                                    "key": "r",
                                    "name": "Debug restart",
                                    "type": "command",
                                    "command": "workbench.action.debug.restart"
                                }
                            ]
                        },
                        {
                            "key": "e",
                            "name": "Editor",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "c",
                                    "name": "Make section",
                                    "type": "command",
                                    "command": "codex.command.makeSection"
                                },
                                {
                                    "key": "d",
                                    "name": "Make divider",
                                    "type": "command",
                                    "command": "codex.command.makeDivider"
                                },
                                {
                                    "key": "l",
                                    "name": "Duplicate line down",
                                    "type": "command",
                                    "command": "editor.action.copyLinesDownAction"
                                },
                                {
                                    "key": "L",
                                    "name": "Duplicate line up",
                                    "type": "command",
                                    "command": "editor.action.copyLinesUpAction"
                                },
                                {
                                    "key": "m",
                                    "name": "Make header",
                                    "type": "command",
                                    "command": "codex.command.makeHeader"
                                },
                                {
                                    "key": "s",
                                    "name": "Toggle header/source",
                                    "type": "command",
                                    "command": "clangd.switchheadersource"
                                }
                            ]
                        },
                        {
                            "key": "s",
                            "name": "Show",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "e",
                                    "name": "Show explorer",
                                    "type": "command",
                                    "command": "workbench.view.explorer"
                                },
                                {
                                    "key": "s",
                                    "name": "Show search",
                                    "type": "command",
                                    "command": "workbench.view.search"
                                },
                                {
                                    "key": "g",
                                    "name": "Show source control",
                                    "type": "command",
                                    "command": "workbench.view.scm"
                                },
                                {
                                    "key": "t",
                                    "name": "Show test",
                                    "type": "command",
                                    "command": "workbench.view.extension.test"
                                },
                                {
                                    "key": "r",
                                    "name": "Show remote explorer",
                                    "type": "command",
                                    "command": "workbench.view.remote"
                                },
                                {
                                    "key": "x",
                                    "name": "Show extensions",
                                    "type": "command",
                                    "command": "workbench.view.extensions"
                                },
                                {
                                    "key": "p",
                                    "name": "Show problem",
                                    "type": "command",
                                    "command": "workbench.actions.view.problems"
                                },
                                {
                                    "key": "o",
                                    "name": "Show output",
                                    "type": "command",
                                    "command": "workbench.action.output.toggleOutput"
                                },
                                {
                                    "key": "d",
                                    "name": "Show debug console",
                                    "type": "command",
                                    "command": "workbench.debug.action.toggleRepl"
                                }
                            ]
                        },
                        {
                            "key": "v",
                            "name": "Versioning",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "m",
                                    "name": "Magit",
                                    "type": "command",
                                    "command": "magit.status"
                                },
                                {
                                    "key": "v",
                                    "name": "Magit",
                                    "type": "command",
                                    "command": "magit.status"
                                }
                            ]
                        },
                        {
                            "key": "?",
                            "name": "Search bindings",
                            "type": "command",
                            "command": "whichkey.searchBindings"
                        },
                        {
                            "key": ".",
                            "name": "Repeat last command",
                            "type": "command",
                            "command": "whichkey.repeatMostRecent"
                        }
                    ],
                    "description": "Whichkey bindings for Code:X"
                },
                "codex.whichkey.bindingOverrides": {
                    "type": "array",
                    "default": [ ],
                    "description": "Whichkey binding overrides for Code:X"
                }
            }
        },
        "configurationDefaults": {
            "emacs-mcx.useMetaPrefixEscape": true,
            "emacs-mcx.useMetaPrefixCtrlLeftBracket": true,
            "emacs-mcx.emacsLikeTab": true
        },
        "keybindings": [
            {
                "key": "ctrl+x j",
                "command": "workbench.action.togglePanel"
            },
            {
                "key": "ctrl+x n",
                "command": "workbench.action.nextEditor"
            },
            {
                "key": "ctrl+x right",
                "command": "-workbench.action.nextEditor"
            },
            {
                "key": "ctrl+x p",
                "command": "workbench.action.previousEditor"
            },
            {
                "key": "ctrl+x left",
                "command": "-workbench.action.previousEditor"
            },
            {
                "key": "alt+g shift+.",
                "command": "cursorBottom",
                "when": "editorTextFocus"
            },
            {
                "key": "escape shift+oem_102",
                "command": "cursorBottom",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+g shift+,",
                "command": "cursorTop",
                "when": "editorTextFocus"
            },
            {
                "key": "escape oem_102",
                "command": "cursorTop",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+s",
                "command": "-emacs-mcx.isearchForward",
                "when": "!findInputFocussed"
            },
            {
                "key": "ctrl+s",
                "command": "swiper.swiper",
                "when": "!findInputFocussed"
            },
            {
                "key": "ctrl+x shift+0",
                "command": "workbench.action.closeEditorsInGroup",
                "when": "!terminalFocus"
            },
            {
                "key": "ctrl+x shift+1",
                "command": "workbench.action.closeEditorsInOtherGroups",
                "when": "!terminalFocus"
            },
            {
                "key": "ctrl+x 5",
                "command": "workbench.action.newWindow"
            },
            {
                "key": "ctrl+x 6",
                "command": "workbench.action.closeWindow"
            },
            {
                "key": "ctrl+x shift+f",
                "command": "workbench.action.files.openFolder"
            },
            {
                "key": "ctrl+x shift+k",
                "command": "workbench.action.closeFolder"
            },
            {
                "key": "ctrl+x left",
                "command": "workbench.action.navigateLeft"
            },
            {
                "key": "ctrl+x right",
                "command": "workbench.action.navigateRight"
            },
            {
                "key": "ctrl+x up",
                "command": "workbench.action.navigateUp"
            },
            {
                "key": "ctrl+x down",
                "command": "workbench.action.navigateDown"
            },
            {
                "key": "ctrl+=",
                "command": "editor.action.fontZoomIn",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+-",
                "command": "editor.action.fontZoomOut",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+0",
                "command": "editor.action.fontZoomReset",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+0",
                "command": "editor.action.fontZoomReset",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+shift+=",
                "command": "workbench.action.zoomIn",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+=",
                "command": "workbench.action.zoomOut",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+0",
                "command": "workbench.action.zoomReset",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+alt+0",
                "command": "workbench.action.zoomReset",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+g",
                "command": "codex.leader.leave",
                "when": "codex.leader.isActive"
            },
            {
                "key": "escape alt+ctrl+space",
                "command": "workbench.action.toggleAuxiliaryBar",
                "when": "config.emacs-mcx.useMetaPrefixEscape"
            },
            {
                "key": "ctrl+x [",
                "command": "workbench.action.toggleSidebarVisibility"
            },
            {
                "key": "ctrl+x ]",
                "command": "workbench.action.toggleAuxiliaryBar"
            },
            {
                "key": "escape q",
                "command": "rewrap.rewrapComment",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+c y",
                "command": "-emacs-mcx.browseKillRing",
                "when": "!terminalFocus"
            },
            {
                "key": "ctrl+c",
                "command": "whichkey.show",
                "args": "codex.whichkey.bindings",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "build": "tsc -p ./",
        "watch": "tsc -p ./ --watch",
        "start": "npm run build",
        "clean": "rm -rf out && rm -rf node_modules && rm -rf *.vsix"
    },
    "devDependencies": {
        "@types/vscode": "^1.94.0",
        "@vscode/vsce": "^3.2.1",
        "ovsx": "^0.9.5",
        "typescript": "^5.7.2"
    },
    "extensionPack": [
        "tuttieee.emacs-mcx",
        "VSpaceCode.whichkey",
        "kahole.magit",
        "wenhoujx.swiper",
        "dnut.rewrap-revived",
        "jwintz.vscodet"
    ]
}